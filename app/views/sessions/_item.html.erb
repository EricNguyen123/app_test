<% @items.each do |item|%>
    <a href="<%=item[:links]%>" class="link_login">
        <div class="signin-button">
            <img class="signin-button-icon" src="/assets/<%= item[:image] %>" alt="Sign in with">
            <span class="signin-button-title">Sign in with <%= item[:name] %> </span>
        </div>
    </a>
<%end%>
